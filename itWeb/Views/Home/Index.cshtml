@{
    ViewBag.Title = "Home Page";
    Layout = null;

    var date = ViewBag.Date;
    var student = ViewBag.Student;

    var list = ViewBag.List;

}

<style>
    div{
        margin-bottom: 10px;
    }
</style>

<div>
    time now is: @date.ToString("yyyy/MM/dd HH:mm:ss")
</div>
<div>
    score:@student.ToString()
</div>

<div>
    Score list
</div>
@for (int i = 0; i < list.Count; i++)
{
    <div>
        @list[i].ToString()
    </div>
}